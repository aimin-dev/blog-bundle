{
    "name": "aimin/blog-bundle",
    "type": "symfony-bundle",
    "description": "Amin Blog Bundle",
    "keywords": [
        "Symfony",
        "Blog"
    ],
    "homepage": "https://github.com/aimin-dev/blog-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Stephan Randriamahenina",
            "email": "m6ahenina@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/asset": "4.4.*",
        "symfony/http-kernel": "4.4.*",
        "symfony/http-foundation": "4.4.*"
    },
    "require-dev": {
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Aimin\\Bundle\\BlogBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "Tests/DependencyInjection"
        }
    }
}
